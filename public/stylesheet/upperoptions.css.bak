/*****************************************************************************/
.navigation1
{
	z-index: 5;
	position: absolute;
	top: -46px;
	left: 1739px;
  margin-left:-10px
}
.navigation1 li
{

  display:inline;
  margin-left:10px;
  position:relative;

  
}
.navigation1 li a
{
  border-radius:0 0 8px 8px;
  color: RGB(10,10,10);
  display:block;
  font-size:18px;
  height:38px;
  line-height:31px;
  padding:42px 16px 0;
  text-decoration:none
}
.navigation1 li:hover a{
  background:transparent url(images/nav_hover.html) no-repeat scroll;

}
.navigation1 li ul
{
  -moz-opacity:0.90;
  -ms-filter:progid:DXImageTransform.Microsoft.Alpha(Opacity=90);
  /*设置导航栏下拉菜单背景颜色*/
  background: white;
  border-radius:5px;
  display:none;
  left:0;
  margin-top:32px;
  opacity:0.90;
  padding-left:5px;
  padding-right:10px;
  position:absolute;
  visibility:hidden;
  z-index:9999
}
.navigation1 li:hover ul
{
  visibility:visible;
}
.navigation1 li ul li
{
	width: 80px;
  border-bottom:1px solid #292929;
  display:block;
  float:none;
  height:40px;
}
.navigation1 li ul li:last-child
{
  border:none
}
.navigation1 li ul li a
{
  background:none!important;
  /*设置导航栏下拉菜单字体颜色*/
  color: black;
  float:none!important;
  font-size:15px;
  height:40px;
  line-height:40px;
  /*这个是选项栏最小大小*/
  min-width:100px;
  padding:0!important;
  width:50%;

}
.navigation1 li ul li a:hover
{

}
.navigation1 li ul li ul
{
  left:203px;
  padding-left:1px;
  padding-top:0;
  top:0;
  visibility:hidden!important
}
.navigation1 li ul li:hover ul
{
  visibility:visible!important
}
.navigation1 li ul .sf-sub-indicator
{
  background:transparent url(images/nav_arrow.html) no-repeat scroll;
  display:block;
  height:10px;
  position:absolute;
  right:6px;
  text-indent:-3000px;
  top:11px;
  width:7px
}
.navigation1 > ul > li > ul{
	  position:absolute;
	  top: 0px;
    transition: opacity 0.5s; /* 设置渐变效果 */
	transition : 0.5s;
    opacity: 0; /* 初始化透明度为0，即不可见 */
}
.navigation1 > ul > li:hover ul{
	  position:absolute;
	  top: 20px;
	opacity: 1; /* 鼠标经过时设置透明度为1，即可见 */
}
.navigation1 > ul > li > ul > li:hover{
	/*悬停下拉菜单背景色*/
	background-color: RGB(235,235,235);
}

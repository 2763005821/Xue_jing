<script>
	function ajax(count,pwd){
		let ht = new XMLHttpRequest( ) 
		ht.open("get",`http://localhost:8976?count=${count}&pwd=${pwd}&func=2`)
		ht.send()
		ht.onreadystatechange = function( ){			
			if(ht.readyState == 4 && ht.status == 200){
			try{
				console.log(JSON.parse(ht.responseText));
				ID=JSON.parse(ht.responseText);
				console.log("ID:");
				console.log(ID.status);				//由服务器传来的ID.status用来判断登录是否成功
				if (ID.status==1)						//创建成功
				{
					alert("创建成功，请重新登录！")
					window.location.href = 'http://localhost:3000/index.html?' + 'uname=' + 'FYC';
				}
				if(ID.status==0)						//创建失败
				{
					new Vue({
						el:"#window_text",
						data:{
							message:"创建失败，已存在账号"
						},
						methods:{
							func1:function(msg){
								this.message="创建失败，已存在账号";
						}
						}
					})
					show();
				}
			}catch(error){
				console.log("无法转换")
			}
			}
		}
}
